    <version>0.5.0</version>
    <description>Defines messages used by the 'node_helpers project.</description>
    <maintainer email="alex@urbanmachine.build">urbanmachine
    </maintainer>
    <license>MIT</license>

    <buildtool_depend>ament_cmake</buildtool_depend>

    <build_depend>rosidl_default_generators</build_depend>
    <build_depend>std_msgs</build_depend>
